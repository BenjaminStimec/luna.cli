{
    "name": "SampleMission",
    "vars": {
        "name_var": {"abc":123}
    },
    "alias": {
        "greet": "sample_kit1.module1.greet"
    },
    "workflows": [
        "sample_kit1.module1.greet($name_var:J{$.abc})=>sample_kit2.module2.save_to_file(%, 'output.txt')",
        "...greet($name_var)=>$abc(%1)=>sample_kit1.module2.capitalize(%1)=>sample_kit2.module1.reverse(%1)=>sample_kit2.module2.save_to_file(%1, '1output.txt')",
	    "...greet('abc')=>sample_kit2.module2.save_to_file(%1, '2output.txt')",
	    "sample_kit1.module1.greet('==> $/abc')=>sample_kit2.module2.save_to_file(%1, '3output.txt')"
    ]
}